{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext",
      "DOM" // "DOM" adds types like console, fetch, Request, Response, etc., which are part of the web environment (Cloudflare Workers uses this).
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
}